<!DOCTYPE html>
<html lang="en" class="h-full">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>nobr | Meet Strangers</title>
</head>
<body class="h-full">
    <header class="w-full shadow-sm">
        <%- include('./components/header', {variant: true}); %>
    </header>
    <main class="flex flex-col h-[80%]">
        <%- include('./components/herosection'); %>
    </main>
    <footer class="absolute -bottom-2 h-[8%] w-full bg-dark-mode-black font-light">
        <%- include('./components/footer'); %>
    </footer>
</body>
</html>
<script>
    document.body.previousElementSibling.insertAdjacentHTML('beforeend', 
        `
        <link rel="shortcut icon" href="/images/header/logo.png">
        <link rel="stylesheet" href="/css/style.css">
        <link rel="stylesheet" href="/font/WEB/css/satoshi.css">
        `)
    document.body.prepend(document.querySelector('header'));
    document.body.append(document.querySelector('footer'));
</script>